body {
  color: black;
  background-color: black;
  font-family: verdana, helvetica, arial, sans-serif;
  font-size: 80%;  /* Enables font size scaling in MSIE */
  margin: 0;
  padding: 0;
}

html > body {
  font-size: 8.5pt;
}

acronym, .titleTip {
  border-bottom: 1px dotted rgb(153,153,153);
  cursor: help;
  margin: 0;
  padding: 0 0 0.4px 0;
}

.doNotDisplay {
  display: none;
}

.smallCaps {
  font-size: 110%;
  font-variant: small-caps;
}


/* ##### Header ##### */

.midHeader {
  color: rgb(39,78,144);
  background-color: rgb(0,0,0);
}

.midHeader .right {
	position: absolute;
	right: 1.5mm;
	top: 0px;
	width: 73px;
	height: 95px;


}


.headerTitle {
  font-size: 337%;
  font-weight: normal;
  margin: 0 0 0 4mm;
  padding: 0.25ex 0;
}

.subHeader {
  color: white;
  background-color:#000000;
  margin: 0;
  padding: 3px 1ex 1ex 1.5mm;

}

.subHeader a {
  color: white;
  background-color: transparent;
  text-decoration: none;
  font-weight: bold;
  margin: 0;
  padding: 2px 5px 2px 5px;
}  

.subHeader a:hover {
  text-decoration: none;
  color: #ff9900;
}

.superHeader .highlight, .subHeader .highlight {
  color :#ff9900;
  background-color: transparent;
}


/* ##### Side Bar ##### */

#side-bar {
  width: 13em;
  float: left;
  clear: left;
 border:0px;
padding: 3px 0px 0px 5px;
background:black;
}

#side-bar div {
  border: 1px solid rgb(153,153,153);
  padding:5px 0px 5px 0px;
 background:#646464;
}
#side-bar form {
  padding:0px 3px 0px 3px;
  font-size:10px;
margin:0 0 0 0;
}

#side-bar table {
  padding:0px 6px 0px 6px;
  font-size:10px;
}



#side-bar p {
 display:block;
width:13em;
padding: 0px 10px 0px 10px;
margin: 0 10px 0.25em 2.5mm;
}

#side-bar p.news {
 display:block;
width:13em;
padding: 3px 0px 2px 8px;
text-align:left;
}
.sideBarTitle {
  font-weight: bold;
  margin: 0 10px 0.25em 2.5mm;
  padding: 0 10px 0 10px;
}

#side-bar ul {
  list-style-type: none;
  list-style-position: outside;
  margin: 0;
  padding: 0 0 1.1em 0;
}

#side-bar li {
  margin: 0;
   padding: 10px 0 0.75em 5mm;
display: block;
}

#side-bar li.sub {
  margin: 0;
   padding: 0px 0 0em 5mm;
display: block;
}
#side-bar a, .thisPage {
  color: rgb(255,255,255);
  background-color: transparent;
  text-decoration: none;
  margin: 0;
  padding: 3px 0 0.75em 1mm;

}

#side-bar a.sublink {
  color: rgb(255,255,255);
  background-color: transparent;
  text-decoration: none;
  margin: 0;
  padding: .5em 0px .5em 7.5mm;
 font-size:9px;
  }

#side-bar a.news {
  color: rgb(0,0,0);
  background-color: transparent;
  text-decoration: none;
 text-align:left;
  margin: 0;
  padding: 0px 0 0em 0mm;

  }

#side-bar a.news:hover {
  color: black;
  text-decoration: none;
}

.thisPage {
  color: black;
  background-color: white;
  padding-left: 4mm;
  border-top: 1px solid rgb(153,153,153);
  border-bottom: 1px solid rgb(153,153,153);
}

#side-bar a:hover {
  color: white;
  text-decoration: underline;
}


#side-bar a.sublink:hover {
  color: white;
  text-decoration: none;
}
.sideBarText {
  line-height: 1.5em;
  margin: 0 0 1em 0;
  padding: 0 1.5ex 0mm;
  display: block;
}

#side-bar .sideBarText a {
  text-decoration: underline;
  margin: 0;
  padding: 0;
  display: inline;
}

#side-bar .sideBarText a:hover {
  color: rgb(0,102,204);
  background-color: transparent;
  text-decoration: none;
}

.lighterBackground {
  color: inherit;
  background-color: white;
}


/* ##### Main Copy ##### */

#main-copy {
  color: white;
  background-color:black;
  text-align: justify;
  line-height: 1.5em;
  margin: 0 0 0 15em;
  padding: 0.5mm 5mm 7mm 5mm;

}

#main-copy p {
  margin: 0.5em 1ex 2em 1ex;
  padding: 0;
}

#main-copy a {
  color: rgb(180,180,180);
  background-color: transparent;
  text-decoration: none;
  padding: 0 0 0 0;
}

#main-copy h1 {
  color: white;
  background-color: rgb(90,90,90);
  font-size: 100%;
  font-weight: bold;
  margin: 0px 0 0 0;
  padding: 0.5ex 0 0.5ex 1ex;
}

#main-copy h3 {
  color: white;
  background-color: transperant;
  font-size: 100%;
  font-weight: bold;
  margin: 0px 0 0 0;
  padding: 0.5ex 5px 0.5ex 5px;
}

#main-copy .topOfPage {
  color: white;
  background-color: transparent;
  font-size: 91%;
  font-weight: bold;
  text-decoration: none;
  margin: 0.5ex 1ex 0 0;  /* For MSIE */
  padding: 0;
  float: right;
}

#main-copy > .topOfPage {
  margin: 2.75ex 1ex 0 0;  /* For fully standards-compliant user agents */
}

#main-copy div
 {
	padding: 10mm 30mm 20mm 30mm;
 }

#main-copy table
 {
	padding: 8px 3px 8px 3px;
	font: 11px verdana;
	color:#ffffff;
	margin: 8px 0 8px 0;
 }

#main-copy img.sponsors
 {
	margin: 5mm 3px 5mm 3px;
	font: 11px verdana;
	color:#ffffff;
 }	
	
dl {
  margin: 1em 1ex 2em 1ex;
  padding: 0;
}

dt {
  font-weight: bold;
  margin: 0 0 0 0;
  padding: 0;
}

dd {
  margin: 0 0 2em 2em;
  padding: 0;
}


/* ##### Footer ##### */

#footer {
  color: white;
  background-color: rgb(64,64,64);
  font-size: 91%;
  margin: 0;
  padding: .5em 2.5mm 1ex 3.5mm;
  clear: both;
}

#footer .right {

  line-height: 1.45em;
  float: right;
  clear: right;

}

#footer .center {
  text-align:center;
  line-height: 1.45em;
}

#footer a {
  color: white;
  background-color: transparent;
  text-decoration: underline;
 padding: 3px 5px 3px 5px;
}

#footer a:hover {
  text-decoration: none;
}
