<script language="JavaScript1.2">
hexinput=255  // Initial color value.
var inc=-1 //increment variable

function fadingtext(){
if(hexinput>0) {
hexinput-=11; // increase color value
document.getElementById("fader").style.color="rgb("+hexinput+","+hexinput+","+hexinput+")"; // Set color value.
setTimeout("fadingtext()",20);
}
else
hexinput=255  //reset hex value
}
function changetext(){
if (!document.getElementById) return
inc++;
if (inc==0)
document.getElementById("fader").innerHTML="<a href=main.php?file=cliffhanger/cliffhanger.php target=midFrame>Cliffhanger</a> - Submission Date - 10th January"
else if (inc==3)
document.getElementById("fader").innerHTML="<a href=main.php?file=chemsplash/technum.php target=midFrame>Poster Presentation</a> - Abstract Submission Date extended to 10th Jan"
else if (inc==2)
document.getElementById("fader").innerHTML="<a href=main.php?file=competitions/opencompetitions.php target=midFrame>Open Competitions</a> - Abstract Submission Date extended to 10th Jan"
else if (inc==4)
document.getElementById("fader").innerHTML="<a href=main.php?file=competitions/competitions.php>Competitions</a> - Registration closes on 10th January"
else if (inc==1)
document.getElementById("fader").innerHTML="<a href=main.php?file=technoholix/coll.php target=midFrame>Colosseum</a> - Biggest PC Gaming Competition in India"
else if (inc==5)
document.getElementById("fader").innerHTML="Registrations for Workshops Closed"
else
{
document.getElementById("fader").innerHTML="<a href=main.php?file=laststraw/laststraw.php target=midFrame>Last Straw</a> - Construction Management";
 inc=-1;
}


fadingtext()
setTimeout("changetext()",1500)
}

window.onload=changetext

</script>
</head>

